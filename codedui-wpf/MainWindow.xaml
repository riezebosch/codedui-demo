<Window x:Class="codedui_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:codedui_wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="2*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" x:Name="input1" AutomationProperties.AutomationId="BAsMDgQKAAAHDwYGBA4JDQ"  />
        <TextBox Grid.Row="1" x:Name="input2" AutomationProperties.AutomationId="BAUCBA8BBgkNDAkEBAMLBg" />
        <Button Grid.Row="2" x:Name="button" Click="button_Click" AutomationProperties.AutomationId="BgkJBA8MAg4MAw0CBAYLBw" />
        <Label Grid.Row="3" x:Name="output" AutomationProperties.AutomationId="CQcKCw0LBQsIAQ0CBAwHAA" />
    </Grid>
</Window>
